}

.flex {
    display: flex;
    justify-content: space-between;
    align-items: flex-start;
    flex-wrap: wrap;
}

.imgs {
    width: 48%;
    height: 250rpx;
    background-color: #eee;
    margin-bottom: 40rpx;
}

.imgs image {
    width: 100%;
    height: 100%;
}